{
  "name": "Vue + Go Dev",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "backend",
  "overrideCommand": true,
  "workspaceFolder": "/app",
  "forwardPorts": [
    8080,
    5173
  ],
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/root/.ssh,type=bind,readonly"
  ],
  "postStartCommand": "bash /app/.devcontainer/start-frontend.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "Vue.volar",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "eamodio.gitlens",
        "cweijan.dbclient-jdbc",
        "cweijan.vscode-database-client2",
        "Postman.postman-for-vscode"
      ]
    }
  }
}